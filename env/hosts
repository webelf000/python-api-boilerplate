python-restapi ansible_ssh_host=127.0.0.1 ansible_ssh_port=2222 ansible_ssh_user=vagrant ansible_ssh_private_key_file=~/.vagrant.d/insecure_private_key
# app-staging ansible_ssh_host=<ip of the staging server>  ansible_ssh_port=22 ansible_ssh_user=<server user> ansible_ssh_private_key_file=<path to the private key>
# app-production ansible_ssh_host=<ip of the production server>  ansible_ssh_port=22 ansible_ssh_user=<server user> ansible_ssh_private_key_file=<path to the private key>